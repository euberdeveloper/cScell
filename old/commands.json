{
    "_arguments": {
        "function": "arguments",
        "arguments": {
            "p-number": {
                "type": "int",
                "alias": "n",
                "default": 3,
                "min": 1
            },
            "q-number": {
                "type": "int",
                "alias": "m",
                "default": 4,
                "min": 1
            },
            "inputs": {
                "type": "array<char*>",
                "alias": "i",
                "default": "NULL"
            },
            "recursive": {
                "type": "bool",
                "alias": "r"
            }
        }
    },
    "init": {
        "function": "init",
        "arguments": {
            "p-number": {
                "type": "int",
                "alias": "n",
                "default": 3,
                "min": 1,
                "description": "The number of P processes. It must be at least 1."
            },
            "q-number": {
                "type": "int",
                "alias": "m",
                "default": 4,
                "min": 1,
                "description": "The number of Q processes. It must be at least 1."
            },
            "inputs": {
                "type": "array<char*>",
                "alias": "i",
                "description": "The inputs as file or directory paths."
            },
            "recursive": {
                "type": "bool",
                "alias": "r",
                "description": "If the directories will be read recusively."
            }
        },
        "description": "Initializes the shit"
    },
    "set": {
        "function": "set",
        "arguments": {
            "p-number": {
                "type": "int",
                "alias": "n",
                "default": 0,
                "min": 1,
                "description": "The number of P processes. It must be at least 1."
            },
            "q-number": {
                "type": "int",
                "alias": "m",
                "default": 0,
                "min": 1,
                "description": "The number of Q processes. It must be at least 1."
            },
            "inputs": {
                "type": "array<char*>",
                "alias": "i",
                "default": "NULL",
                "description": "The inputs as file or directory paths"
            },
            "recursive": {
                "type": "bool",
                "alias": "r",
                "description": "If the directories will be read recusively"
            },
            "keep": {
                "type": "bool",
                "alias": "k",
                "description": "If instead of reset the precedent inputs the new inputs will be only added"
            }
        },
        "description": "Initializes the shit"
    },
    "restart": {
        "function": "restart",
        "arguments": {
            "p-number": {
                "type": "int",
                "alias": "n",
                "default": 0,
                "min": 1
            },
            "q-number": {
                "type": "int",
                "alias": "m",
                "default": 0,
                "min": 1
            },
            "inputs": {
                "type": "array<char*>",
                "alias": "i",
                "default": null
            },
            "recursive": {
                "type": "bool",
                "alias": "r"
            }
        }
    },
    "pop": {
        "function": "pop",
        "arguments": {
            "inputs": {
                "type": "array<char*>",
                "alias": "i"
            }
        }
    },
    "status": {
        "function": "status"
    },
    "stop": {
        "function": "stop"
    },
    "show": {
        "function": "show",
        "arguments": {
            "sensitive": {
                "type": "bool",
                "alias": "s"
            },
            "percentage": {
                "type": "bool",
                "alias": "p"
            },
            "detailed": {
                "type": "bool",
                "alias": "d"
            },
            "total": {
                "type": "bool",
                "alias": "t"
            },
            "files": {
                "type": "array<char*>",
                "alias": "f",
                "default": "NULL"
            }
        }
    },
    "quit": {
        "function": "quit"
    }
}